{
    "name": "haas-application",
    "version": "1.0.0",
    "description": "CI build manager for virtual machines in vSphere",
    "main": "index.js",
    "scripts": {
        "prod:build:client": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./src/webpack/webpack.config.prod.js",
        "prod:build:server": "cross-env NODE_ENV=production tsc -p ./src/server",
        "prod:build": "rimraf build && gulp prepare && yarn run prod:build:server && yarn run prod:build:client",
        "prod:run": "cross-env NODE_ENV=production node build/server/server.js",
        "dependencies": "yarn && gulp prepare",
        "dev:build": "rimraf build && tsc -p ./src/server",
        "dev": "rimraf build && docker-compose up -d && cross-env NODE_ENV=development tsc-watch -p ./src/server --onSuccess \"node ./build/server/server.js\"",
        "clean": "rimraf build && rimraf .awcache && rimraf .localStorage",
        "apitest:run": "rimraf build && docker-compose up -d && cross-env NODE_ENV=apitesting tsc-watch -p ./src/server --onSuccess \"node ./build/server/server.js\"",
        "apitest:test": "mocha -r ts-node/register test/e2e/*.test.ts"
    },
    "keywords": [
        "vmware",
        "node",
        "react",
        "CI"
    ],
    "author": "Geoff Rich",
    "license": "MIT",
    "repository": {
        "type": "git",
